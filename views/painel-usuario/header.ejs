<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"><%= breadcrumb.at(-1).titulo %></h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    
                    <% for(let i = 0; i < breadcrumb.length; i++) { %>
                        <% if (i != breadcrumb.length - 1) { %>
                            <li class="breadcrumb-item"><a href="<%= breadcrumb[i].rota %>"><%= breadcrumb[i].titulo %></a></li>
                        <% } else { %>
                            <li class="breadcrumb-item active"><%= breadcrumb[i].titulo %></li>
                        <% } %>
                    <% } %>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div><!-- /.content-header -->
